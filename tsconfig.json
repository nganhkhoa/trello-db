{
   "compilerOptions": {
      "experimentalDecorators": true,
      "emitDecoratorMetadata": true,
      "moduleResolution": "node",
      "allowJs": true,
      "target": "es6",
      "module": "commonjs",
      "lib": [
         "es6",
         "dom"
      ],
      "baseUrl": "./src",
      "paths": {
         "@/*": ["*"]
      }
   },
   "exclude": [
      "node_modules"
   ],
   "include": [
      "src/**/*"
   ]
   
}
